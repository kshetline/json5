{
  "name": "json5",
  "version": "2.3.0",
  "description": "JSON for Humans",
  "main": "lib/index.js",
  "module": "dist/index.mjs",
  "bin": "lib/cli.js",
  "browser": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "dist/"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rollup -c",
    "build-package": "node build/package.js",
    "build-unicode": "node build/unicode.js",
    "coverage": "mkdir -p coverage && nyc report --reporter=text-lcov > coverage/lcov.info",
    "lint": "eslint --fix .",
    "lint-report": "eslint .",
    "prepublishOnly": "npm run production",
    "preversion": "npm run production",
    "production": "run-s test build",
    "mocha": "nyc --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter=text --reporter=html mocha test/**/*.js",
    "test": "run-s lint-report mocha",
    "version": "npm run build-package && git add package.json5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/json5/json5.git"
  },
  "keywords": [
    "json",
    "json5",
    "es5",
    "es2015",
    "ecmascript"
  ],
  "author": "Aseem Kishore <aseem.kishore@gmail.com>",
  "contributors": [
    "Max Nanasy <max.nanasy@gmail.com>",
    "Andrew Eisenberg <andrew@eisenberg.as>",
    "Jordan Tucker <jordanbtucker@gmail.com>",
    "Kerry Shetline <kerry@shetline.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/json5/json5/issues"
  },
  "homepage": "http://json5.org/",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "chai": "^4.3.6",
    "core-js": "^2.6.11",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "regenerate": "^1.4.0",
    "rollup": "^4.41.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^20.0.0",
    "unicode-10.0.0": "^0.7.5"
  }
}
